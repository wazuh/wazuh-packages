  chmod 640 ${SYS_DIR}/sysctl.d/${NAME}.conf
  chmod 640 ${SYS_DIR}/systemd/system/${NAME}.service
  chmod 640 ${SYS_DIR}/systemd/system/${NAME}-performance-analyzer.service
